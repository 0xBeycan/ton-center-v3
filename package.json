{
    "name": "ton-center-v3",
    "version": "0.1.3",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        },
        "./response": {
            "import": "./dist/response.js",
            "require": "./dist/response.js"
        }
    },
    "files": [
        "src",
        "dist",
        "README.md",
        "!tsconfig.tsbuildinfo"
    ],
    "scripts": {
        "build": "tsc",
        "test": "jest",
        "typecheck": "tsc --noEmit",
        "format": "prettier --write .",
        "lint": "eslint . --fix --ext .ts",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "web3",
        "crypto",
        "ton",
        "blockchain",
        "standard",
        "the open network"
    ],
    "author": "0xBeycan",
    "license": "MIT",
    "homepage": "https://github.com/0xBeycan/ton-center-v3/tree/master/packages/utils",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/0xBeycan/ton-center-v3.git"
    },
    "bugs": {
        "url": "https://github.com/0xBeycan/ton-center-v3/issues"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "eslint": "8.57.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-config-standard-with-typescript": "43.0.1",
        "eslint-plugin-prettier": "5.1.3",
        "globals": "^15.14.0",
        "jest": "^29.7.0",
        "prettier": "^3.4.2",
        "ts-jest": "^29.2.5",
        "typescript": "^5.7.3"
    },
    "dependencies": {
        "axios": "^1.7.9"
    }
}
